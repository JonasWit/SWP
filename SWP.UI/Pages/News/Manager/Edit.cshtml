@page "{id:int?}"
@model SWP.UI.Pages.News.Manager.EditModel
@{
    int? postId = null;

    if (RouteData.Values.ContainsKey("id"))
    {
        postId = int.Parse(RouteData.Values["id"].ToString());
    }
}

<component type="typeof(NewsAdmin)" render-mode="Server"  param-UserId="@Model.ActiveUserId" param-PostId="@postId"/>